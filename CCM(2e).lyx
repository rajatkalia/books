\end_layout

\begin_layout Quote
b) 
\begin_inset Formula $f\left(x\right)=\ln\left(1-2x\right)$
\end_inset


\end_layout

\begin_layout Quote
c) 
\begin_inset Formula $f\left(x\right)=\dfrac{1}{e^{2x}}$
\end_inset


\end_layout

\begin_layout Quote
d) 
\begin_inset Formula $f\left(x\right)=\dfrac{\ln x}{e^{4x}}$
\end_inset


\end_layout

\begin_layout Quote
e) 
\begin_inset Formula $f\left(x\right)=\ln2x.e^{2x}$
\end_inset


\end_layout

\begin_layout Quote
f) 
