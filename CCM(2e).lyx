\size footnotesize
\begin_inset Formula $V_{cone}=\dfrac{1}{3}\pi\left(\dfrac{r}{\sqrt{2}}\right)^{2}.\dfrac{r}{\sqrt{2}}$
\end_inset


\end_layout

\begin_layout Standard

\size footnotesize
\begin_inset Formula $V_{portion}=v-V_{cone}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Graphics
