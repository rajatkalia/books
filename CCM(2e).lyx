\end_inset


\end_layout

\begin_layout Standard

\size footnotesize
\begin_inset Formula $V_{portion}=\dfrac{2}{3}\pi r^{3}\left(1-\frac{1}{\sqrt{2}}\right)-\dfrac{1}{3}\pi\left(\dfrac{r}{\sqrt{2}}\right)^{2}.\dfrac{r}{\sqrt{2}}$
\end_inset


\end_layout

\begin_layout Standard

\size footnotesize
\begin_inset Formula $V_{portion}$
\end_inset

=
\begin_inset Formula $\frac{1}{3}\pi r^{3}\left(2-\frac{5}{4}\sqrt{2}\right)$
