\end_inset


\end_layout

\begin_layout Quote
g) 
\begin_inset Formula $f\left(x\right)=\dfrac{e^{x}+e^{-x}}{e^{x}-e^{-x}}$
\end_inset


\end_layout

\begin_layout Description
Sol.
 a) Let 
\begin_inset Formula $g(x)=e^{x}$
\end_inset


\end_layout

\begin_layout Quote
Then 
\begin_inset Formula $f(x)=g(3x-3)$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}(x)=g^{/}\left(3x-3\right).3$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}(x)=e^{3x-3}.3=3e^{3x-3}$
\end_inset


\end_layout

\begin_layout Quote
b) 
\begin_inset Formula $f^{/}\left(x\right)=\dfrac{1}{1-2x}.(-2)=\dfrac{-2}{1-2x}$
\end_inset


\end_layout

\begin_layout Quote
c) Now 
\begin_inset Formula $f(x)$
\end_inset

 can be written in a simpler form i.e.
 
\begin_inset Formula $f\left(x\right)=e^{-2x}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}(x)=e^{-2x}.(-2)=-2e^{-2x}$
\end_inset


\end_layout

\begin_layout Quote
d) 
\begin_inset Formula $f^{/}(x)=\dfrac{e^{4x}.\dfrac{1}{x}-\ln x.e^{4x}.4}{\left(e^{4x}\right)^{2}}=\dfrac{1-x\ln x.4}{xe^{4x}}$
\end_inset


\end_layout

\begin_layout Quote
e) 
\begin_inset Formula $f^{/}\left(x\right)=\ln2x.e^{2x}.2+\dfrac{1}{2x}e^{2x}.2=e^{2x}\left(2\ln2x+\dfrac{1}{x}\right)$
\end_inset


\end_layout

\begin_layout Quote
f)
\begin_inset Formula $f^{/}(x)=cosecx.\ln x.\dfrac{d}{dx}x^{3}+cosecx.x^{3}.\dfrac{d}{dx}\ln x+\ln x.x^{3}\dfrac{d}{dx}cosecx$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=cosecx.\ln x.3x^{2}+cosecx.x^{3}.\dfrac{1}{x}+\ln x.x^{3}.\left(-cosecx.\cot x\right)$
\end_inset


\end_layout

\begin_layout Quote
g) 
\size footnotesize

\begin_inset Formula $f^{/}\left(x\right)=\dfrac{\left(e^{x}-e^{-x}\right)\dfrac{d}{dx}\left(e^{x}+e^{-x}\right)-\left(e^{x}+e^{-x}\right)\dfrac{d}{dx}\left(e^{x}-e^{-x}\right)}{\left(e^{x}-e^{-x}\right)^{2}}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=\dfrac{\left(e^{x}-e^{-x}\right)\left(e^{x}-e^{-x}\right)-\left(e^{x}+e^{-x}\right)\left(e^{x}+e^{-x}\right)}{\left(e^{x}-e^{-x}\right)^{2}}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=\dfrac{-4}{\left(e^{x}-e^{-x}\right)^{2}}$
\end_inset


\end_layout

\begin_layout Section
The Chain Rule
\end_layout

\begin_layout Standard
If there exists a composite function y = 
\begin_inset Formula $f(g(x))$
\end_inset

 .
 Then 
\begin_inset Formula $\dfrac{dy}{dx}$
\end_inset

 can be expressed in a more convenient form i.e 
\begin_inset Formula $\dfrac{dy}{dx}=\dfrac{df(g(x))}{dg(x)}.\dfrac{dg(x)}{dx}$
\end_inset

 .
 Ofcourse, the composite function can be further branched.
 In that case, the chain would become longer.
\end_layout

\begin_layout Standard
A slightly easier to understand definition also exists viz 
\begin_inset Formula $\dfrac{dy}{dx}=\dfrac{dy}{du}.\dfrac{du}{dv}.\dfrac{dv}{dx}$
\end_inset


\end_layout

\begin_layout Standard
The proposition would be more clear with a few examples
\end_layout

\begin_layout Itemize
Example 1 : 
\begin_inset Formula $y=\sin\left(x^{3}\right)$
\end_inset


\begin_inset Separator latexpar
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\Rightarrow y=f\left(g\left(x\right)\right)$
\end_inset

where 
\begin_inset Formula $f\left(x\right)=\sin\left(x\right)$
\end_inset

and 
\begin_inset Formula $g\left(x\right)=x^{3}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=\cos x$
\end_inset

 and 
\begin_inset Formula $g^{/}\left(x\right)=3x^{2}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Rightarrow\dfrac{dy}{dx}=\dfrac{df(g(x))}{dg(x)}.\dfrac{dg(x)}{dx}=f^{/}\left(g\left(x\right)\right).g^{/}\left(x\right)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Rightarrow\dfrac{dy}{dx}=\cos\left(x^{3}\right).3x^{2}$
\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
Example 2 : 
\begin_inset Formula $y=\left(\tan\left(\left(\ln x\right)^{3}\right)\right)^{2}$
\end_inset


\begin_inset Separator latexpar
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\Rightarrow y=f\left(g\left(h\left(k\left(x\right)\right)\right)\right)$
\end_inset

 where 
\begin_inset Formula $f\left(x\right)=x^{2}$
\end_inset

 , 
\begin_inset Formula $g\left(x\right)=\tan x$
\end_inset

 , 
\begin_inset Formula $h(x)=x^{3}$
\end_inset

 and 
\begin_inset Formula $k\left(x\right)=\ln x$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Rightarrow\dfrac{dy}{dx}=f^{/}\left(g\left(h\left(k\left(x\right)\right)\right)\right).g^{/}\left(h\left(k\left(x\right)\right)\right).h^{/}\left(k\left(x\right)\right).k^{/}\left(x\right)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Rightarrow\dfrac{dy}{dx}=2\tan\left(\left(\ln x\right)^{3}\right).\sec^{2}\left(\left(\ln x\right)^{3}\right).3\left(\ln x\right)^{2}.\dfrac{1}{x}$
\end_inset


\end_layout

\end_deeper
